<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#C1A988</item>
        <item name="colorPrimaryDark">#A08C70</item>
        <item name="colorAccent">#b85135</item>
        <item name="android:windowBackground">#F4F1EA</item>
    </style>


    <style name="CalendarDateText">
        <item name="android:textColor">#b85135</item>
    </style>
    <style name="LabelTitle" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- 定义标题文字大小 -->
        <item name="android:labelTextSize">50dp</item>
        <item name="android:textStyle">bold</item>
        <item name="colorPrimary">#C1A988</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:windowBackground">#FDF6E6</item>

    </style>
    <style name="ButtonStytle" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- 定义标题文字大小 -->
        <item name="iconSize">50dp</item>
        <item name="icon">@color/white</item>
        <item name="android:windowBackground">#FFFCF4</item>

    </style>

</resources>